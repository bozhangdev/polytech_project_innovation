<!DOCTYPE html>
<html>
<head>
    <title>All my items</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
<%include index.ejs%>
<div class="body">
<div id="header2">
    <div id="item-published">
        <table>
            <tr>
                <th>ITEM ID</th>
                <th>NAME</th>
                <th>PRICE</th>
                <th>STATUS</th>
                <th>DESCRIPTION</th>
            </tr>
            <%searchresult.forEach(function(item) { %>
            <tr>
                <td><a><%= item.id%></a></td>
                <td><%= item.name%></td>
                <td><%= item.price %></td>
                <td><%= item.status %></td>
                <td><%= item.description %></td>
            </tr>
            <% })%>
        </table>

    </div>
</div>
</div>
<script type="text/javascript" src="/javascripts/opennav.js"></script>

</body>
</html>